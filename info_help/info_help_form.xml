<?xml version= "1.0"?>
<openerp>
	<data>
		<!-- add form view -->
		<record id="view_form_info_help" model="ir.ui.view">
			<field name="name">Info Help Form</field>
			<field name="model">info.help</field>
			<field name="arch" type="xml">
				<form string="Info Help">
					<sheet>
						<group>
							<field name="base_intro"/>
							<field name="func_easy" />
						</group>
					</sheet>
				</form>
			</field>
		</record>
		<!-- add action -->
		<act_window id="action_info_help"
			name="Info Help"
			res_model="info.help"
			view_mode = "list,form"
			target="current"
			domain="[('id','=','1')]"
			/>
		<!-- add menuitem -->
		<menuitem id="menu_info_help_main"
			name="Info Help"
			sequence = "5"
			parent= "mail.mail_feeds_main"/>
		<menuitem id="menu_info_help_video"
			name="Help Videos"
			parent="menu_info_help_main"
			sequence = "10"
			action = "action_info_help"
			/>
	</data>
</openerp>
